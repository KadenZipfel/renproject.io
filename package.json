{
    "name": "renproject.io",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
        "@fortawesome/free-brands-svg-icons": "^5.1.0-11",
        "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
        "@fortawesome/react-fontawesome": "0.1.0-11",
        "@types/react-router-dom": "^4.3.0",
        "axios": "^0.18.0",
        "i": "^0.3.6",
        "node-sass": "^4.9.2",
        "node-sass-chokidar": "^1.3.3",
        "npm": "^6.3.0",
        "npm-run-all": "^4.1.3",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "react-headroom": "^2.2.2",
        "react-reveal": "^1.2.2",
        "react-router-dom": "^4.3.1",
        "react-scripts-ts": "^4.0.8",
        "react-slick": "^0.23.2",
        "ts-debounce": "^0.3.2"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "start-js": "react-scripts-ts start",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts-ts build",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject",
        "deploy": "npm run build; echo 'renproject.io' > build/CNAME; git add build -f; git commit -m 'Deploying to renproject.io'; git push origin `git subtree split --prefix build master`:gh-pages --force"
    },
    "devDependencies": {
        "@types/jest": "^23.3.1",
        "@types/node": "^10.5.5",
        "@types/react": "^16.4.7",
        "@types/react-dom": "^16.0.6",
        "typescript": "^3.0.1"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
